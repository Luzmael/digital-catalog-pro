{
  "version": 2,
  "builds": [
    {
      "src": "tiendas/_template/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/data/(.*)",
      "dest": "/tiendas/_template/data/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600",
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/public/(.*)",
      "dest": "/tiendas/_template/public/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/tiendas/_template/index.html"
    }
  ]
}
