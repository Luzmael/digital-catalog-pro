{
  "version": 2,
  "builds": [
    {
      "src": "tiendas/**/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/tiendas/([^/]+)/data/(.*\.json)",
      "headers": {
        "Content-Type": "application/json",
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/tiendas/(.*)",
      "dest": "/tiendas/_template/index.html"
    }
  ]
}
