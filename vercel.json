{
  "version": 2,
  "builds": [
    {
      "src": "tiendas/_template/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/data/(.*)",
      "dest": "tiendas/_template/data/$1",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/public/(.*)",
      "dest": "tiendas/_template/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "tiendas/_template/index.html"
    }
  ]
}
